{
  "expo": {
    "name": "Nest Bank",
    "slug": "nest-bank",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/Nest_Bank_logo.png",
    "scheme": "nestbankv3",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/images/Nest_Bank_logo.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.princeprabhat.nestbank",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Nest_Bank_logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.princeprabhat.nestbank"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/Nest_Bank_logo.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#16161f",
          "image": "./assets/images/Nest_Bank_logo.png",
          "dark": {
            "image": "./assets/images/Nest_Bank_logo.png",
            "backgroundColor": "#16161f"
          },
          "imageWidth": 400
        }
      ],
      "expo-router",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "56d9ef1b-439c-406a-be10-ac7fe822cb79"
      }
    }
  }
}
